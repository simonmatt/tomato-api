<service>
	<id>nodetomatoapi.exe</id>
	<name>node-tomato-api</name>
	<description>Tomato API for Angular, Vue and React</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>G:\Projects\NodeJS\node-tomato-api\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>G:\Projects\NodeJS\node-tomato-api\app.js</argument>
	<argument>--log</argument>
	<argument>node-tomato-api wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<workingdirectory>G:\Projects\NodeJS\node-tomato-api</workingdirectory>
</service>